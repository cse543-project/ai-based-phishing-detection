{
  "name": "PhishGuard",
  "version": "1.0",
  "description": "AI-powered phishing detection extension",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/main.png",
      "48": "icons/main.png",
      "128": "icons/main.png"
    }
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "http://localhost:8000/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}